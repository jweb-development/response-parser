    "url": "git+https://github.com/jweb-development/response-parser.git"
  },
  "bugs": {
    "url": "https://github.com/jweb-development/response-parser/issues"
  },
  "keywords": [
    "JWEB",
    "HTTP",
    "HTTP Codes",
    "HTTP Responses"
  ],
  "homepage": "https://github.com/jweb-development/response-parser#readme",
  "author": "JWEB Development LLC",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^14.6.0"
  },
  "devDependencies": {
    "prettier": "^2.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7"
  }
}
